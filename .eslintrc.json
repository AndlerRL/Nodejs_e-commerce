{
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "prefer-const": [
      "warn"
    ],
    "no-duplicate-imports": [
      "error"
    ],
    "no-unused-vars": [
      "error", 
      { "vars": "all", "args": "none", "ignoreRestSiblings": false }
    ],
    "camelcase": [
      "error",
      { "properties": "never" }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}